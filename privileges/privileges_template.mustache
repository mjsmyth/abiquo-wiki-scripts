<ac:macro ac:name="section">
  <ac:rich-text-body>
    <ac:macro ac:name="div">
      <ac:parameter ac:name="class">landsc smallcode</ac:parameter>
      <ac:rich-text-body>
        <table>
          <tbody>
          {{#categories}}
            <tr>
              <td class="highlight info" data-highlight-class="info">
                <p><h6>{{categoryName}} Privileges</h6></p>
              </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
            </tr>
            <tr>
              <th>
                <p>GUI Label <span style="color: rgb(239,239,239);">_________________</span>
                </p>
              </th>
              <th>
                <p>Application Tag</p>
              </th>
              <th>
                <p>Privilege<span style="color: rgb(239,239,239);">____________________________________</span>
                </p>
              </th>
              <th class="warning" data-highlight-class="warning">
                <p>Cloud Admin</p>
              </th>
              <th class="note" data-highlight-class="note">
                <p>Ent Admin</p>
              </th>
              <th class="success" data-highlight-class="success">
                <p>Ent User</p>
              </th>
              <th class="info" data-highlight-class="info">
                <p>Outbound API</p>
              </th>
              <th>
                <p>Info</p>
              </th>
            </tr> 
            {{#entries}}  
            <tr>
              <td>{{guiLabel}}</td>
              <td>{{applicationTag}}</td>
              <td>{{privilege}}</td>
              <td class="highlight warning" data-highlight-class="warning"><p>{{cloudAdmin}}</p></td>
              <td class="highlight note" data-highlight-class="note"><p>{{entAdmin}}</p></td>
              <td class="highlight success" data-highlight-class="success"><p>{{entUser}}</p></td>
              <td class="highlight info" data-highlight-class="info"><p>{{outboundApi}}</p></td>
              {{#info}}
                <td><ac:emoticon ac:name="yellow-star"/></td>
              {{/info}}
              {{^info}}
                <td> </td>
              {{/info}}
            </tr>
            {{/entries}}
          {{/categories}}   
          </tbody>
        </table>
      </ac:rich-text-body>
    </ac:macro>
  </ac:rich-text-body>
</ac:macro>