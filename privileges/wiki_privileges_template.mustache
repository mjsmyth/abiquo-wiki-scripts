<ac:macro ac:name="section">
  <ac:rich-text-body>
    <ac:macro ac:name="div">
      <ac:parameter ac:name="class">landsc smallcode</ac:parameter>
      <ac:rich-text-body>
        <table>
          <tbody>
          {{#categories}}
            <tr>
              <td class="highlight info" data-highlight-class="info">
                <p><h6>{{category}} privileges</h6></p>
              </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
              <td class="highlight info" data-highlight-class="info"> </td>
            </tr>
            <tr>
              <th>
                <p>GUI Label <span style="color: rgb(239,239,239);">_________________</span></p>
              </th>
              <th>
                <p>Application Tag</p>
              </th>
              <th>
                <p>Privilege<span style="color: rgb(239,239,239);">____________________________________</span></p>
              </th>
              {{#roleheader}}
              <th class="{{roleheadform}}" data-highlight-class="{{roleheadform}}"><p>{{rolename}}</p></th>
              {{/roleheader}}
              <th><p>Info</p></th>
            </tr> 
            {{#entries}}  
            <tr>
              <td>{{guiLabel}}</td>
              <td>{{appTag}}</td>
              <td>{{privilege}}</td>
              {{#roles}}
              {{#role}}
              <td class="highlight {{roleformat}}" data-highlight-class="{{roleformat}}"><p>
              {{#rolehas}}
              {{roleMark}}</p>
              {{/rolehas}}
              {{^rolehas}}
              </p>
              {{/rolehas}}
              </td>
              {{/role}}
              {{/roles}}
              {{#info}}
                <td><ac:emoticon ac:name="yellow-star"/></td>
              {{/info}}
              {{^info}}
                <td> </td>
              {{/info}}
            </tr>
            {{/entries}}
          {{/categories}}   
          </tbody>
        </table>
      </ac:rich-text-body>
    </ac:macro>
  </ac:rich-text-body>
</ac:macro>
